<h1>All Wikis</h1>
<div class="container">
<div class="row">
  <div class="col-md-6">
<% @wikis.each do |wiki| %>
    <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <p><%= link_to wiki.title, wiki %></p>
       </h4>
     </div>
    </div>
  </div>
   <% end %>

  <div class="col-md-4">
    <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
  </div>
</div>
</div>
